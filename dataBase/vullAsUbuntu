COPY KOPPEL_14 FROM '$PWD/../vullDataBase' DELIMITER '%' NULL AS 'NULL'\"" \
&& echo helloThere \
&& cd ../realwrldTest \
&& cp proTijdelijk ../vullDataBase \
&& cd ../dataBase \
&& echo $token | sudo -S su - postgres -c "psql `cat .postSettings` <<< \"
COPY EIWIT_14 FROM '$PWD/../vullDataBase' DELIMITER '#' NULL AS 'NULL'\""\
