&& cp genIDLength ../vullDataBase \
&& cd ../dataBase \
&& echo $token | sudo -S su - postgres -c "psql `cat .postSettings` <<< \"
COPY GEN_14 FROM '$PWD/../vullDataBase' DELIMITER ' ' NULL AS 'NULL'\"" \
